{% extends "base.twig" %}
{% block title %}
    SlimBlog
{% endblock %}
{% block content %}
    <div class="row">
        {% for post in posts %}
            <div class="col-md-12">
                <h4>
                    <a href="{{ path_for('posts.displaySingle', { id: post.id }) }}">{{ post.title }}</a>
                </h4>
            </div>
        {% endfor %}
    </div>
{% endblock %}